#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules/pkgroll/dist/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules/pkgroll/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules/pkgroll/dist/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules/pkgroll/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/pkgroll@2.5.1/node_modules:/home/yugz/domains/web-dev/better-meanings/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
